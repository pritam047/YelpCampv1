<%- include('partials/header') %>

  <div class="container">
      <div class="row">
        
        <div style="width:40%;margin:0px auto">
            <h1 style="text-align: center;">Add A New Landscape</h1>
            <form action="/campgrounds" method="post">
                <div class="form-group">
                    <input class="form-control" type="text" name="name" placeholder="Add name">
                </div>
                <div class="form-group">
                    <input class="form-control" type="number" name="price" placeholder="Add price" min="0.01" step="0.01">
                </div>
                <div class="form-group">
                    <input class="form-control" type="text" name="image" placeholder="Add image URL">
                </div>  
                <div class="form-group">
                  <input class="form-control" type="text" name="desc" placeholder="Add description">
              </div>
                <div class="form-group" style="text-align: center;">
                    <button class="btn btn-lg btn-primary">SUBMIT</button>
                </div>
            </form>
            <a href="/campgrounds" style="text-align: center;position: relative;">Go Back</a>
        </div>
      </div>

  </div>




<%- include('partials/footer') %>